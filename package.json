  },
  "scripts": {
    "start": "json-server --watch db.json & ./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node src/server.js",
    "dev": "json-server --watch db.json & ./node_modules/.bin/babel-node src/server.js",
    "build": "./node_modules/.bin/babel src -d dist & ./node_modules/.bin/cross-env NODE_ENV=production PLATFORM=web ./node_modules/.bin/webpack --config config/webpack.config.prod.js",
    "start-prod": "node dist/server.js",
    "test": "./bin/test"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest-shim.js",
      "<rootDir>/config/jest-setup.js"
    ]
  },
  "devDependencies": {
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
